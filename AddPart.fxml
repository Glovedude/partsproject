<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddPart">
   <children>
      <RadioButton fx:id="outsourced" layoutX="300.0" layoutY="83.0" mnemonicParsing="false" onAction="#Outsourced" text="Outsourced">
         <toggleGroup>
            <ToggleGroup fx:id="addPart" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="inHouse" layoutX="166.0" layoutY="83.0" mnemonicParsing="false" onAction="#inHouse" selected="true" text="In-House" toggleGroup="$addPart" />
      <Label layoutX="49.0" layoutY="83.0" text="Add Part" underline="true">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label layoutX="82.0" layoutY="176.0" text="ID" />
      <Label layoutX="82.0" layoutY="223.0" text="Name" />
      <Label layoutX="82.0" layoutY="252.0" text="Inv" />
      <Label layoutX="82.0" layoutY="284.0" text="Cost/Price" />
      <Label layoutX="82.0" layoutY="314.0" text="Max" />
      <TextField fx:id="name" layoutX="193.0" layoutY="219.0" />
      <TextField fx:id="inv" layoutX="193.0" layoutY="248.0" />
      <TextField fx:id="price" layoutX="193.0" layoutY="280.0" />
      <TextField fx:id="max" layoutX="193.0" layoutY="310.0" />
      <Label layoutX="355.0" layoutY="314.0" text="Min" />
      <TextField fx:id="min" layoutX="397.0" layoutY="310.0" />
      <TextField fx:id="machineId" layoutX="193.0" layoutY="338.0" />
      <Button fx:id="save" layoutX="340.0" layoutY="448.0" mnemonicParsing="false" onAction="#save" text="Save" />
      <Button fx:id="cancel" layoutX="397.0" layoutY="448.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Label fx:id="radioTitle" layoutX="82.0" layoutY="342.0" prefHeight="17.0" prefWidth="104.0" text="Machine ID" />
      <TextField fx:id="iD" disable="true" layoutX="193.0" layoutY="172.0" promptText="Auto Gen - Disabled" />
   </children>
</AnchorPane>
